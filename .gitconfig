[user]
    # Set these in ~/.gitconfig.local
    # name = Your Name
    # email = your.email@example.com

[credential]
	helper = osxkeychain

[core]
	editor = code --wait
	autocrlf = false
	excludesfile = /Users/__USERNAME__/.gitignore
	eol = lf

[init]
	defaultBranch = main

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
